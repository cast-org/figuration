{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": "../.eslintrc.json",
  "rules": {
    "func-style": "off",
    "no-magic-numbers": "off",
    "node/no-process-exit": "off",
    "node/shebang": "off",
    "no-process-exit": "off",
    "strict": "off"
  }
}